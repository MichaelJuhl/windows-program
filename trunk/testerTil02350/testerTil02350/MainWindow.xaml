<Window x:Class="testerTil02350.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="02350 - Windowsprograming Using C# and .NET" Height="350" Width="525">
    <Grid>
        <StackPanel  HorizontalAlignment="Left" Margin="0,26,0,23" Name="stackPanel1" VerticalAlignment="Stretch" Width="75" >
            <Label Content="Vælg og klik" Height="28" Name="labelTopStackPanel1" HorizontalContentAlignment="Center" />
            <Button Content="Class" Height="23" Name="buttonClass" Width="75" Margin="0,0,0,0" Click="button_Click" ToolTip="Click here for Class"/>
            <Button Content="Arrow" Height="23" Name="buttonArrow" Width="75" Click="button_Click" ToolTip="Click here for Arrow"/>
            <Button Content="Note" Height="23" Name="buttonNote" Width="75" Click="button_Click" ToolTip="Click here for Note"/>
            <Expander Header="Class" Height="Auto" MaxHeight="74" Name="expander1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0" ToolTip="Click here for Class" Expanded="expander1_Expanded" AllowDrop="True">
                <Grid Height="125">
                    <TextBox Text="Name" FontSize="10" FontStyle="Italic" HorizontalContentAlignment="Center" Height="23" HorizontalAlignment="Left" Margin="0,0,0,0" Name="textBox1" VerticalAlignment="Top" Width="75" />
                    <Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="0,23,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button_Click"/>
                </Grid>
            </Expander>
            <Expander Header="Arrow" Height="Auto" MaxHeight="74" HorizontalAlignment="Left" VerticalAlignment="Top" Name="expander2" Margin="0,0,0,0" IsExpanded="False" ToolTip="Click here for Arrow" Expanded="expander2_Expanded">
                <Grid Height="125">
                    <TextBox Text="Name" FontSize="10" FontStyle="Italic" HorizontalContentAlignment="Center" Height="23" HorizontalAlignment="Left" Margin="0,0,0,0" Name="textBox2" VerticalAlignment="Top" Width="75" />
                    <Button Content="Add" Height="23" HorizontalAlignment="Left" Margin="0,23,0,0" Name="button2" VerticalAlignment="Top" Width="75" Click="button_Click"/>
                </Grid>
            </Expander>
        </StackPanel>
        <Menu Height="26" HorizontalAlignment="Stretch" Name="menu1" VerticalAlignment="Top" Margin="0,0,0,0" > 
            <MenuItem Header="_Menu" Click="MenuItem_Click" >
                <MenuItem Header="_Open" />
                <MenuItem Header="_Save" />
                <MenuItem Header="_Exit" />
            </MenuItem>
            <MenuItem Header="_Edit" Click="MenuItem_Click">
                <MenuItem Header="_Copy" />
                <MenuItem Header="_Cut" />
                <MenuItem Header="_Paste" />
                <MenuItem Header="_UnDo" />
                <MenuItem Header="_ReDo" />
            </MenuItem>
            
            <MenuItem Header="_Help" Click="MenuItem_Click">
                <MenuItem Header="_Help" />
                <MenuItem Header="_About" />
            </MenuItem>
        </Menu>
        <StatusBar Height="23" HorizontalAlignment="Stretch" Margin="0,0,0,0" Name="statusBar1" VerticalAlignment="Bottom" />
        <ScrollViewer CanContentScroll="True"   Margin="75,26,100,23" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible">
